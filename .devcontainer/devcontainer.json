{
	"name": "gcp python in remote container",
  "dockerComposeFile": "../docker-compose.yml",
  "service": "app",
  "workspaceFolder": "/src",
  "shutdownAction": "stopCompose",
  "customizations": {
    "vscode": {
      "settings": {
        "http.proxyStrictSSL": false
      },
      "extensions": ["ms-python.python", "ms-python.vscode-pylance"]
    }
  }
}
